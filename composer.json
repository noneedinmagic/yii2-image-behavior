{
    "name": "noneedinmagic/yii2-image-behavior",
    "description": "Small extension which adds useful ImageBehavior to easily maintain images linked to the Model and easily create their thumbnails on the fly",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Tron",
            "email": "noneedinmagic@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*"
    },
    "autoload": {
        "psr-4": {
            "noneedinmagic\\images\\": ""
        }
    },
    "repositories":[
        {
            "type": "git",
            "url": "https://github.com/noneedinmagic/yii2-image-behavior.git"
        }
    ]
}
